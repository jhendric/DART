&perfect_model_obs_nml
   async = 0,
   adv_ens_command = "./advance_ens.csh",
   obs_seq_in_file_name = "obs_seq.in",
   obs_seq_out_file_name = "obs_seq.out",
   start_from_restart = .false.,
   output_restart = .false.,
   restart_in_file_name = "perfect_ics",
   restart_out_file_name = "perfect_restart",
   init_time_days = 148074,
   init_time_seconds = 43200,
   output_interval = 1  /

&filter_nml
   async = 0,
   adv_ens_command = "./advance_ens.csh",
   ens_size = 20, 
   start_from_restart = .false.,
   output_restart = .false.,
   obs_sequence_in_name = "obs_seq.out",
   obs_sequence_out_name = "obs_seq.final",
   restart_in_file_name = "filter_ics",
   restart_out_file_name = "filter_restart", 
   init_time_days = 148074,
   init_time_seconds = 43200,
   output_state_ens_mean = .true.,
   output_state_ens_spread = .true.,
   output_obs_ens_mean = .true.,
   output_obs_ens_spread = .true., 
   num_output_state_members = 10,
   num_output_obs_members = 0,
   output_interval = 1,
   num_groups = 1,
   outlier_threshold = -1.0 /

&ensemble_manager_nml
   in_core = .true.,
   single_restart_file_in = .true.,
   single_restart_file_out = .true. /

&assim_tools_nml
   filter_kind = 1,
   cutoff = 1000.,
   sort_obs_inc = .false.,
   do_parallel = 0,
   num_domains = 1,
   parallel_command = "./assim_filter.csh",
   spread_restoration = .false.,
   internal_outlier_threshold = -1.0, 
   sampling_error_correction = .false.,
   num_close_threshold = 10000000 /

&adaptive_inflate_nml
   do_obs_inflate = .false.,
   do_varying_ss_inflate = .false.,
   do_single_ss_inflate = .false.,
   start_from_inflate_restart = .false.,
   output_restart = .true., 
   deterministic_inflate = .true.,
   inflate_in_file_name = "inflate_ics",
   inflate_out_file_name = "inflate_restart",
   obs_inf_initial = 1.0,
   obs_inf_sd_initial = 0.0,
   obs_sd_lower_bound = 0.0,
   obs_inf_upper_bound = 1000000.0,
   ss_inf_initial = 1.0,
   ss_inf_sd_initial = 0.0,
   ss_sd_lower_bound = 0.0,
   ss_inf_upper_bound = 1000000.0,
   diagnostic_file_name = "inflate_diag" /

&cov_cutoff_nml
   select_localization = 2  /

&reg_factor_nml
   select_regression = 1,
   input_reg_file = "time_mean_reg" 
   save_reg_diagnostics = .false.,
   reg_diagnostics_file = 'reg_diagnostics'  /

&obs_sequence_nml
   write_binary_obs_sequence = .false.  /

&obs_kind_nml
   assimilate_these_obs_types = 'RADIOSONDE_TEMPERATURE',
                                'RADIOSONDE_U_WIND_COMPONENT',
                                'RADIOSONDE_V_WIND_COMPONENT',
                                'RADIOSONDE_SPECIFIC_HUMIDITY',
                                'METAR_U_10_METER_WIND',
                                'METAR_V_10_METER_WIND',
                                'METAR_TEMPERATURE_2_METER',
                                'METAR_SPECIFIC_HUMIDITY_2_METER',
                                'METAR_SURFACE_PRESSURE'  /

&assim_model_nml
   write_binary_restart_files = .true. /

&location_nml
   horiz_dist_only = .true.,
   vert_normalization_pressure = 100000.0, 
   vert_normalization_height = 10000.0,
   vert_normalization_level = 20.0,
   approximate_distance = .false. /

&utilities_nml
   TERMLEVEL = 1,
   logfilename = 'dart_log.out'  /

&preprocess_nml
    input_obs_kind_mod_file = '../../../obs_kind/DEFAULT_obs_kind_mod.F90',
   output_obs_kind_mod_file = '../../../obs_kind/obs_kind_mod.f90',
     input_obs_def_mod_file = '../../../obs_def/DEFAULT_obs_def_mod.F90',
    output_obs_def_mod_file = '../../../obs_def/obs_def_mod.f90',
   input_files              = '../../../obs_def/obs_def_reanalysis_bufr_mod.f90',
                              '../../../obs_def/obs_def_metar_mod.f90' /

&obs_diag_nml
   obs_sequence_name = 'obs_seq.final',
   first_bin_center =  2006, 6, 1, 0, 0, 0,
   last_bin_center  =  2006, 6, 2, 0, 0, 0,
   bin_separation   =     0, 0, 0, 1, 0, 0,
   bin_width        =     0, 0, 0, 1, 0, 0,
   time_to_skip     =     0, 0, 0, 0, 0, 0,
   max_num_bins  = 1000,
   mlevel     = 5,
   plevel     = 500,
   hlevel     = 5000,
   obs_select = 1,
   Nregions   = 1,
   rat_cri    = 3.0,
   qc_threshold = 4.0,
   lonlim1    =   0.0,
   lonlim2    = 360.0,
   latlim1    = -90.0,
   latlim2    =  90.0,
   reg_names  = 'global',
   print_mismatched_locs = .false.  /

&merge_obs_seq_nml
   filename_seq1 = 'obs_seq.one',
   filename_seq2 = 'obs_seq.two',
   filename_out  = 'obs_seq.merged'  /

&model_nml
   num_est_params = 0,
   est_param_types = 604,
   pert_init_sd = 0.01,
   pert_param_sd = 0.0,
   pert_init_beta_1 = -1,
   pert_init_beta_2 = -1,
   pert_param_min = 0.00000001,
   pert_param_max = 0.99999999,
   maintain_initial_spread = .false.,
   dist_shape = 'beta', 
   real_obs_period = 1800, 
   start_real_obs = 1800  /
 
