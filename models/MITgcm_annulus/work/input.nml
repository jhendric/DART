                                'RADIOSONDE_U_WIND_COMPONENT',
                                'RADIOSONDE_V_WIND_COMPONENT'
   evaluate_these_obs_types   = 'RADIOSONDE_SURFACE_PRESSURE' /

&assim_model_nml
